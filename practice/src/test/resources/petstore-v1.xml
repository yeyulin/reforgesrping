<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="petStore"
          class="org.practice.service.v1.PetStoreService">
    </bean>
    <bean id="petStoreSingleton"
          class="org.practice.service.v1.PetStoreService" scope="singleton">
    </bean>
    <bean id="petStorePrototype"
          class="org.practice.service.v1.PetStoreService" scope="prototype">
    </bean>
    <bean id="invalidBean"
          class="xxx.xxxxx">
    </bean>
</beans>

